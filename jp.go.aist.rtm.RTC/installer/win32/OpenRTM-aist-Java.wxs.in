                  <File Id="{% file.Id %}"
                       Name="{% file.Name %}" DiskId="1"
                       Source="{% file.Source %}">
                    <Shortcut Id="{% file.Id %}" Directory="ExpMenuFolder"
                              Name="Start orbd"
                              Show="normal" WorkingDirectory="bin"/>
                  </File>
                  {% endif %}
                {% endfor %}
              </Component>
            </Directory>

